@using System
@using System.Globalization
@using System.Linq
@using System.Linq.Expressions
@using RazorLight
@using TownClub.Reports.Source
@inherits TemplatePage<TownClub.Reports.Source.Minutes>
@{
    DisableEncoding = true;
}
<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>@Model.Title</title>
    <meta name="author" content="@Model.KeeperOfMinutes">

    <!-- LIBS -->
    <script src="libs/pagedjs/paged.polyfill.js"></script>

    <!-- STYLES -->
    <link rel="stylesheet" href="styles/default.css">
    <style>
        @@page {
            size: A4;
            margin-top: 25mm;
            margin-right: 25mm;
            margin-bottom: 20mm;
            margin-left: 25mm;

            @@bottom-center
            {
                content: "Page " counter(page) " of " counter(pages);
                font-family: "Noticia Text";
                font-size: .8em;
                font-weight: bold;
            }

            @@bottom-right
            {
                content: "Version @Model.Version from @Model.VersionDate.ToString("d", CultureInfo.InvariantCulture)";
                font-family: "Noticia Text";
                font-size: .8em;
                font-weight: bold;
                line-height: 1.4em;
            }

            @@bottom-left
            {
                content: "@Model.Signature";
                white-space: pre;
                font-family: "Noticia Text";
                font-size: .8em;
                font-weight: bold;
                line-height: 1.4em;
            }
        }

        @@page:first
        {
            @@bottom-left
            {
                content: "Art: (T)ask, (V)ote, (S)tatement";
                font-family: "Noticia Text";
                font-size: .8em;
                line-height: 1.4em;
            }
        }
    </style>
</head>
<body>
    <h1>@Model.Title</h1>
    <subtitle>@Model.Subtitle (@Model.Signature)</subtitle>
    <img id="logo" src="data:image/png;base64," />

    <!-- HEADER -->
    <table class="head">
        <tr>
            <td class="label">Date</td>
            <td>@Model.Date.ToString("d", CultureInfo.InvariantCulture)</td>
            <td class="label">Time</td>
            <td>@Model.Time</td>
        </tr>
        <tr>
            <td class="label">Venue</td>
            <td>@Model.Venue</td>
            <td class="label">Duration</td>
            <td>@Model.Duration</td>
        </tr>
        <tr>
            <td class="label">Chairman</td>
            <td>@Model.Chairman</td>
            <td class="label">Keeper o.M.</td>
            <td>@Model.KeeperOfMinutes</td>
        </tr>
        <tr>
            <td class="label">Attendees</td>
            <td colspan="3">

                <ul class="signaturen">
                    @foreach (var member in Model.Attendees.OfType<Member>().Where(m => !m.Absent))
                    {
                        <li>
                            <span class="title">@member.Name</span>
                            <span class="signatur">@member.Signature</span>
                        </li>
                    }
                </ul>

            </td>
        </tr>
        <tr>
            <td class="label">Absent</td>
            <td colspan="3" class="shaded">

                <ul class="signaturen">
                    @foreach (var member in Model.Attendees.OfType<Member>().Where(m => m.Absent))
                    {
                        <li>
                            <span class="title">@member.Name</span>
                            <span class="signatur">@member.Signature</span>
                        </li>
                    }
                </ul>

            </td>
        </tr>
        <tr>
            <td class="label">Guests</td>
            <td colspan="3">
                @if (!Model.Attendees.OfType<Guest>().Any())
                {
                    <span>---keine---</span>
                }
                else
                {
                    <ul class="signaturen">
                        @foreach (var guest in Model.Attendees.OfType<Guest>())
                        {
                            <li>
                                <span class="title">@guest.Name</span>
                                <span class="signatur">@guest.Signature</span>
                            </li>
                        }
                    </ul>
                }
            </td>
        </tr>
    </table>

    <!-- TOPS -->
    <h2>Agenda</h2>
    <ol>
        <li class="head">Topic</li>
        @foreach (var item in Model.Agenda)
        {
            <li>@item.Title</li>
        }
    </ol>

    <!-- ERGEBNISSE -->
    <h2>Records</h2>
    <ol class="results">
        <li class="head">
            <p>
                <span class="meta"><span class="art">Type</span><span class="vw">Resp.</span><span class="frist">Deadl.</span></span>
                Votes / Actions / Notes
            </p>
        </li>
        @foreach (var item in @Model.Agenda)
        {
            <li>
                @foreach (var action in item.Actions)
                {
                    <p>
                        <span class="meta">
                            <span class="art">
                                @if (action.Type != "I")
                                {
                                    @action.Type
                                }
                            </span>
                            <span class="vw @if(String.IsNullOrWhiteSpace(action.Responsibility)) { @Raw("leer"); }">@action.Responsibility</span>
                            <span class="frist">@action.Deadline</span>
                        </span>

                        @action.Text
                    </p>
                }
            </li>
        }
    </ol>

    <!-- NÄCHSTE SITZUNG -->
    @if (!String.IsNullOrWhiteSpace(Model.NextMeeting))
    {
        <div class="naechste-sitzung">
            <p>Next Meeting:</p>
            <p><strong>@Model.NextMeeting</strong></p>
        </div>
    }

    <!-- UNTERSCHRIFTEN -->
    <unterschriften>
        <ort-datum>
            <p>&nbsp;</p>
            <label>Place and Date</label>
        </ort-datum>

        @foreach (var signature in Model.Signatures)
        {
            <unterschrift>
                <label class="name">@signature.Name</label>
                <label class="funktion">@signature.Role</label>
            </unterschrift>
        }
    </unterschriften>

    <!-- ANHÄNGE -->
    <h2>Attachments</h2>
    <ol class="anhaenge">
        <li class="head">Title, Notes</li>
        @if (!Model.Attachments.Any())
        {
            <span>--- none ---</span>
        }
        else
        {
            @foreach (var attachment in Model.Attachments)
            {
                <li><span class="signatur">@attachment.Signature</span>@attachment.Title</li>
            }
        }
    </ol>
</body>
</html>